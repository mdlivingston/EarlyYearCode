	java -classpath bin parser.Parser iterator.max

iterationx:
	dpl iterator.max

functions:
	java -classpath bin parser.Parser functions.max

functionsx:
	dpl functions.max

dictionary:
	java -classpath bin parser.Parser dictionary.max

dictionaryx:
	dpl dictionary.max

problem:
	java -classpath bin parser.Parser rpnCalculator.max

